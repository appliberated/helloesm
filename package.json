{
    "name": "helloesm",
    "version": "1.0.0",
    "description": "Can your browser handle ECMAScript Modules (aka native JavaScript modules)?",
    "scripts": {
        "workbox": "workbox generateSW"
    },
    "keywords": [],
    "author": "HWALab",
    "license": "MIT",
    "eslintConfig": {
        "env": {
            "browser": true,
            "es6": true
        },
        "extends": "eslint:all",
        "parserOptions": {
            "sourceType": "module"
        },
        "rules": {
            "func-style": "off",
            "func-names": "off",
            "capitalized-comments": ["error", "always", { "ignorePattern": "helloesm" }],
            "linebreak-style": ["error", "windows"],
            "max-len": ["error", 120],
            "padded-blocks": "off",
            "space-before-function-paren": "off"
        }
    }
}